<UserControl x:Class="ImageStitchingSystem.Resource.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImageStitchingSystem.Resource"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Line X1="0" Y1="150" X2="145" Y2="150" Stroke="Black" StrokeThickness="1"/>
        <Line X1="150" Y1="0" X2="150" Y2="145" Stroke="Black" StrokeThickness="1"/>

        <Line X1="155" Y1="150" X2="300" Y2="150" Stroke="Black" StrokeThickness="1"/>
        <Line X1="150" Y1="155" X2="150" Y2="300" Stroke="Black" StrokeThickness="1"/>
    </Grid>
</UserControl>
